/**register.wxss**/
page {
  height: 100vh;
  display: flex;
  flex-direction: column;
}

.input {
  box-sizing: border-box;
  height: 80rpx;
  background-color: #fff;
  box-shadow: 0 0 10rpx 5rpx #00000020;
}

.container-this {
  height: 100%;
  display: flex;
  flex-direction: column;
  padding: 100rpx 20rpx;
  box-sizing: border-box;
}

.center {
  align-self: center;
}

.nav {
  align-items: center;
  gap: 50rpx;
}

.trantext {
  font-size: 32rpx;
  font-weight: bold;
  color: #4a4a4a;
}