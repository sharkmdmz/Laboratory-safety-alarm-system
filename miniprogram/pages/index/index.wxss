page {
  height: 100vh;
  display: flex;
  flex-direction: column;
}

.container-this {
  height: 100%;
  display: flex;
  flex-direction: column;
  padding: 100rpx 20rpx;
  box-sizing: border-box;
}

.nav {
  text-align: center;
  margin-top: 15px;
  color: #007aff; /* 注册链接颜色 */
  cursor: pointer; /* 鼠标悬停时呈现手形 */
  text-decoration: underline; /* 下划线效果 */
}

.flex-h{
  justify-content: space-between;
  margin:20rpx 40rpx 0 40rpx;
}