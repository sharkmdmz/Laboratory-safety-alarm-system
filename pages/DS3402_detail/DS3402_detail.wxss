.detail-page {
  background-color: #f9f9f9;
  min-height: 100vh;
}

.lab-number {
  background-color: #007aff;
  color: white;
  text-align: center;
  padding: 15px;
  font-size: 24px;
  font-weight: bold;
}

.two-columns {
  display: flex;
  padding: 20px;
}

.info-column {
  flex: 1;
  padding: 10px;
  border: 1px solid #ddd;
  border-radius: 5px;
  margin: 0 10px;
  background-color: white;
}

.column-title {
  font-size: 18px;
  font-weight: bold;
  margin-bottom: 10px;
}

.info-item {
  margin-bottom: 5px;
}
  